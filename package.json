{
    "name": "apollo",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "update-test": "npm run test -- -u",
        "build": "npx webpack   --config ./webpack.config.prod.babel.js",
        "start": "npx webpack-dev-server --config ./webpack.config.dev.babel.js",
        "analyze": "npx webpack --config ./webpack.config.analyze.babel.js --profile --json > stats.json",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "graphql-codegen": "npx graphql-codegen --config codegen.yml"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "jest": {
        "collectCoverage": true,
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.js",
            "jest-localstorage-mock"
        ],
        "transformIgnorePatterns": [
            "/node_modules/(?!@material-ui)",
            "/node_modules/(?!@babel)"
        ],
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest"
        },
        "moduleNameMapper": {
            "@/(.*)": "<rootDir>/src/$1",
            "database": "<rootDir>/src/lib/database"
        }
    },
    "dependencies": {
        "@apollo/client": "^3.0.2",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "graphql": "^15.3.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-select": "^3.1.0",
        "react-slick": "^0.27.1",
        "recharts": "^1.8.5",
        "slick-carousel": "^1.8.1",
        "styled-components": "^5.1.1",
        "use-debounce": "^3.4.3"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@babel/node": "^7.10.5",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-transform-block-scoping": "^7.10.5",
        "@babel/plugin-transform-classes": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@graphql-codegen/cli": "1.17.7",
        "@graphql-codegen/introspection": "1.17.7",
        "@graphql-codegen/typescript": "^1.17.7",
        "@graphql-codegen/typescript-operations": "1.17.7",
        "@graphql-codegen/typescript-react-apollo": "1.17.7",
        "@svgr/webpack": "^5.4.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-loader-exclude-node-modules-except": "^1.0.3",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-plugin-import": "^1.13.0",
        "babel-plugin-styled-components": "^1.10.7",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^4.0.0",
        "dotenv-webpack": "^2.0.0",
        "error-overlay-webpack-plugin": "^0.4.1",
        "eslint": "^7.5.0",
        "eslint-loader": "^4.0.2",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "jest": "^26.1.0",
        "path": "^0.12.7",
        "prettier": "^2.0.5",
        "style-loader": "^1.2.1",
        "stylelint": "^15.10.1",
        "stylelint-config-recommended": "^3.0.0",
        "typescript": "^3.9.7",
        "webpack": "^4.44.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.2.2"
    }
